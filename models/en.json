{
  "invocation": "my test app",
  "version": "4.0",
  "intents": {
    "ReserveTableIntent": {
      "phrases": [
        "reserve a table",
        "book a table",
        "table reservation",
        "I want to reserve a table",
        "I want to book a table"
      ]
    },
    "ExampleIntent": {
      "phrases": ["example", "show me an example"]
    },
    "HelpIntent": {
      "phrases": ["what can I do", "help"]
    },
    "NumberIntent": {
      "phrases": ["{number}"],
      "entities": {
        "number": {
          "nlpjs": "number"
        }
      }
    },
    "YesIntent": {
      "phrases": ["yes", "yes please", "sure"]
    },
    "NoIntent": {
      "phrases": ["no", "no thanks"]
    }
  }
}
